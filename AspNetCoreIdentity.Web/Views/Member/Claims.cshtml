
@{
    ViewData["Title"] = "Claims";
}
@model List<ClaimViewModel>
<h1>Claims</h1>


<table class="table">
  <thead>
    <tr>
      <th scope="col">Issuer</th>
      <th scope="col">Type</th>
      <th scope="col">Value</th>
    </tr>
  </thead>
  <tbody>

      @foreach (var item in Model)
        {
           <tr>

              <td>@item.Issuer</td>
              <td>@item.Type</td>
              <td>@item.Value</td>       
             </tr>   
        }
 
  </tbody>
</table>